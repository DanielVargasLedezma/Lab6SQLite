<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.lab6sqlite">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/_079310"
            android:label="@string/app_name"
            android:roundIcon="@drawable/_079310"
            android:supportsRtl="true"
            android:theme="@style/Theme.Lab6SQLite">
        <activity
                android:name=".ui.view.CrearUsuarioActivity"
                android:exported="true"
                android:theme="@style/Theme.Lab6SQLite.NoActionBar"/>
        <activity
                android:name=".ui.view.NavDrawActivity"
                android:exported="true"
                android:label="@string/title_activity_nav_draw"
                android:theme="@style/Theme.Lab6SQLite.NoActionBar"/>
        <activity
                android:name=".ui.view.MainActivity"
                android:exported="true"
                android:theme="@style/Theme.Lab6SQLite.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>